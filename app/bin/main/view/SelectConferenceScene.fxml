<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="302.0" prefWidth="589.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SelectConferenceController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <TableView fx:id="conferencesTable" layoutX="7.0" layoutY="12.0" prefHeight="256.0" prefWidth="585.0">
              <columns>
                <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Name" />
                <TableColumn fx:id="conferenceDateColumn" prefWidth="120.0" text="Conference Date" />
                  <TableColumn fx:id="submissionDateColumn" prefWidth="142.4000244140625" text="Paper Submission Date" />
                  <TableColumn fx:id="venueColumn" prefWidth="58.39996337890625" text="Venue" />
                  <TableColumn fx:id="reviewersColumn" prefWidth="186.400146484375" text="Reviewers" />
              </columns>
            </TableView>
            <Label fx:id="selectConferenceLabel" layoutX="44.0" layoutY="268.0" text="Select a Conference to submit papers to, then press Save to record the selection, then press Next" />
            <Button fx:id="saveButton" layoutX="259.0" layoutY="315.0" mnemonicParsing="false" onAction="#onButtonPressedSave" text="Save" />
            <Button fx:id="nextButton" layoutX="300.0" layoutY="315.0" mnemonicParsing="false" onAction="#onButtonPressedNext" text="Next" />
         </children>
      </Pane>
   </children>
</AnchorPane>
